<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.osf.trans.mapper.CodeMapper">
	<select id="getCodeList" resultType="code">
		select * from code
	</select>
	<select id="getCodeByCode" resultType="code">
		select * from code where code_num = #{codeNum}
	</select>
</mapper>